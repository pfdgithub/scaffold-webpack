{
  "name": "scaffold-webpack",
  "version": "1.0.0",
  "description": "脚手架，基于 webpack 搭建。",
  "scripts": {
    "check": "gulp check",
    "clean": "gulp clean",
    "latest": "gulp copyLatest",
    "compress": "gulp compress",
    "serve": "gulp serve --env=dev",
    "build:dev": "gulp build --env=dev",
    "build:test": "cross-env NODE_ENV=production gulp build --env=test",
    "build:prod": "cross-env NODE_ENV=production gulp build --env=prod"
  },
  "scaffoldConfig": {
    "deploy": {
      "portalMultiVersion": false,
      "portalMinifyHtml": false,
      "portalBeautifyHtml": true,
      "assetMultiVersion": true,
      "assetNameHash": false,
      "assetExtractCss": true,
      "enablePwa": false,
      "manifest": {
        "name": "Webpack 脚手架",
        "short_name": "脚手架",
        "description": "脚手架，基于 webpack 搭建。",
        "start_url": "//[host]/[path]/?launcher=homescreen"
      }
    },
    "dev": {
      "port": 8000,
      "https": false,
      "devtool": "cheap-module-source-map",
      "rpc": {
        "innerMode": "mock",
        "innerPrefix": "//[host]/[path]/"
      }
    },
    "test": {
      "pagePrefix": "//[host]/[path]/",
      "assetPrefix": "//[host]/[path]/",
      "rpcPrefix": {
        "inner": "//[host]/[path]/"
      }
    },
    "prod": {
      "pagePrefix": "//[host]/[path]/",
      "assetPrefix": "//[host]/[path]/",
      "rpcPrefix": {
        "inner": "//[host]/[path]/"
      }
    }
  },
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/pfdgithub/scaffold-webpack"
  },
  "homepage": "https://github.com/pfdgithub/scaffold-webpack",
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-imports": "^0.2.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "cache-loader": "^1.2.0",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "del": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "fancy-log": "^1.3.2",
    "file-loader": "^1.1.6",
    "gulp": "^3.9.1",
    "gulp-zip": "^4.1.0",
    "happypack": "^4.0.0",
    "hash-all-modules-plugin": "^0.1.0",
    "html-loader": "^0.5.4",
    "html-webpack-event-plugin": "^0.1.1",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.30.1",
    "js-beautify": "^1.7.5",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "multer": "^1.3.0",
    "name-all-modules-plugin": "^1.0.1",
    "open": "^0.0.5",
    "plugin-error": "^1.0.0",
    "postcss-loader": "^2.0.10",
    "shelljs": "^0.8.0",
    "style-loader": "^0.19.1",
    "svg-inline-loader": "^0.8.0",
    "svg-sprite-loader": "^3.6.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0",
    "webpack-pwa-manifest": "^3.4.1",
    "workbox-sw": "^2.1.2",
    "workbox-webpack-plugin": "^2.1.2",
    "write-file-webpack-plugin": "^4.2.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.5",
    "core-js": "^2.5.3",
    "natty-fetch": "^2.5.3",
    "natty-storage": "^2.2.0",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  }
}
