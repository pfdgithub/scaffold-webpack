{
  "name": "scaffold-webpack",
  "version": "1.0.0",
  "description": "脚手架，基于 webpack 搭建。",
  "scripts": {
    "check": "gulp check",
    "clean": "gulp clean",
    "imagemin": "gulp imagemin",
    "compress": "gulp compress",
    "serve": "cross-env NODE_ENV=development gulp serve --env=dev",
    "build:dev": "cross-env NODE_ENV=development gulp build --env=dev",
    "build:test": "cross-env NODE_ENV=production gulp build --env=test",
    "build:prod": "cross-env NODE_ENV=production gulp build --env=prod",
    "deploy": "cross-env NODE_ENV=production gulp build"
  },
  "scaffold": {
    "deploy": {
      "portalMultiVersion": false,
      "portalMinifyHtml": false,
      "portalBeautifyHtml": true,
      "portalChunkHash": false,
      "assetMultiVersion": false,
      "assetNameHash": true,
      "assetExtractCss": true,
      "enablePwa": true,
      "enablePush": false,
      "manifest": {
        "name": "Webpack 脚手架",
        "short_name": "脚手架",
        "description": "脚手架，基于 webpack 搭建。"
      }
    },
    "dev": {
      "port": 8000,
      "https": false,
      "historyApi": false,
      "devtool": "cheap-module-eval-source-map",
      "rpcMode": "mock",
      "rpcPrefix": {
        "inner": "https://[host]/[path]/"
      },
      "deploy": {
        "assetNameHash": false,
        "assetExtractCss": false,
        "enablePwa": false
      }
    },
    "test": {
      "pagePrefix": "https://[host]/[path]/",
      "assetPrefix": "https://[host]/[path]/",
      "rpcPrefix": {
        "inner": "https://[host]/[path]/"
      },
      "deploy": {}
    },
    "prod": {
      "pagePrefix": "https://[host]/[path]/",
      "assetPrefix": "https://[host]/[path]/",
      "rpcPrefix": {
        "inner": "https://[host]/[path]/"
      },
      "deploy": {}
    }
  },
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/pfdgithub/scaffold-webpack"
  },
  "homepage": "https://github.com/pfdgithub/scaffold-webpack",
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-imports": "^0.2.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "cache-loader": "^1.2.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "del": "^3.0.0",
    "eslint": "^5.4.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-react": "^7.11.1",
    "fancy-log": "^1.3.2",
    "file-loader": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-zip": "^4.2.0",
    "happypack": "^5.0.0",
    "hash-all-modules-plugin": "^0.2.1",
    "html-loader": "^0.5.5",
    "html-webpack-event-plugin": "^0.2.0",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "js-beautify": "^1.7.5",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "multer": "^1.3.1",
    "plugin-error": "^1.0.1",
    "postcss-loader": "^3.0.0",
    "shelljs": "^0.8.2",
    "style-loader": "^0.22.1",
    "svg-inline-loader": "^0.8.0",
    "svg-sprite-loader": "^3.9.2",
    "url-loader": "^1.1.1",
    "web-push": "^3.3.2",
    "webpack": "^4.17.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-command": "^0.4.1",
    "webpack-dev-server": "^3.1.5",
    "webpack-pwa-manifest": "^3.7.1",
    "workbox-sw": "^3.4.1",
    "workbox-webpack-plugin": "^3.4.1",
    "write-file-webpack-plugin": "^4.3.2",
    "yargs": "^12.0.1"
  },
  "peerDependencies": {
    "gulp-imagemin": "^4.1.0"
  },
  "dependencies": {
    "antd": "^3.6.3",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.6",
    "core-js": "^2.5.7",
    "natty-fetch": "^2.5.6",
    "natty-storage": "^2.2.0",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "raf": "^3.4.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.5",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  }
}
