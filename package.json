{
  "name": "scaffold-webpack",
  "version": "1.0.0",
  "description": "脚手架，基于 webpack 搭建。",
  "scripts": {
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "tsc:build": "tsc --emitDeclarationOnly",
    "check": "gulp check",
    "clean": "gulp clean",
    "imagemin": "gulp imagemin",
    "compress": "gulp compress",
    "server": "cross-env NODE_ENV=development gulp server --env=dev",
    "build:dev": "cross-env NODE_ENV=development gulp build --env=dev",
    "build:test": "cross-env NODE_ENV=production gulp build --env=test",
    "build:prod": "cross-env NODE_ENV=production gulp build --env=prod",
    "deploy": "cross-env NODE_ENV=production gulp build"
  },
  "scaffold": {
    "deploy": {
      "portalMultiVersion": false,
      "portalMinifyHtml": false,
      "portalBeautifyHtml": false,
      "portalChunkHash": false,
      "assetMultiVersion": false,
      "assetNameHash": true,
      "assetExtractCss": true,
      "enablePwa": false,
      "enablePush": false,
      "manifest": {
        "name": "Webpack 脚手架",
        "short_name": "脚手架",
        "description": "脚手架，基于 webpack 搭建。"
      }
    },
    "dev": {
      "port": 8000,
      "https": false,
      "historyApi": false,
      "devtool": "cheap-module-eval-source-map",
      "headers": {},
      "rpcMode": "mock",
      "rpcPrefix": {
        "inner": "https://[host]/[path]/"
      },
      "deploy": {
        "assetNameHash": false,
        "assetExtractCss": false,
        "enablePwa": false
      }
    },
    "test": {
      "pagePrefix": "https://[host]/[path]/",
      "assetPrefix": "https://[host]/[path]/",
      "rpcPrefix": {
        "inner": "https://[host]/[path]/"
      },
      "deploy": {}
    },
    "prod": {
      "pagePrefix": "https://[host]/[path]/",
      "assetPrefix": "https://[host]/[path]/",
      "rpcPrefix": {
        "inner": "https://[host]/[path]/"
      },
      "deploy": {}
    }
  },
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/pfdgithub/scaffold-webpack"
  },
  "homepage": "https://github.com/pfdgithub/scaffold-webpack",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-proposal-optional-catch-binding": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-private-methods": "^7.3.4",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "autoprefixer": "^9.4.9",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-imports": "^0.2.4",
    "body-parser": "^1.18.3",
    "cache-loader": "^2.0.1",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "del": "^4.0.0",
    "eslint": "^5.15.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-react": "^7.12.4",
    "fancy-log": "^1.3.3",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "gulp": "^4.0.0",
