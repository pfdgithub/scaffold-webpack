    "yargs-parser": "^13.0.0"
  },
  "peerDependencies": {
    "gulp-imagemin": "^4.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime-corejs2": "^7.3.4",
    "classnames": "^2.2.6",
    "core-js": "^2.6.5",
    "natty-fetch": "^2.6.0",
    "natty-storage": "^2.3.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-hot-loader": "^4.7.2",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  }
}
