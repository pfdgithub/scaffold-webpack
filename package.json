{
  "name": "scaffold-webpack",
  "version": "1.0.0",
  "description": "脚手架，基于 webpack 搭建。",
  "scripts": {
    "check": "gulp check",
    "clean": "gulp clean",
    "imagemin": "gulp imagemin",
    "compress": "gulp compress",
    "serve": "cross-env NODE_ENV=development gulp serve --env=dev",
    "build:dev": "cross-env NODE_ENV=development gulp build --env=dev",
    "build:test": "cross-env NODE_ENV=production gulp build --env=test",
    "build:prod": "cross-env NODE_ENV=production gulp build --env=prod",
    "deploy": "cross-env NODE_ENV=production gulp build"
  },
  "scaffold": {
    "deploy": {
      "portalMultiVersion": false,
      "portalMinifyHtml": false,
      "portalBeautifyHtml": false,
      "portalChunkHash": false,
      "assetMultiVersion": false,
      "assetNameHash": true,
      "assetExtractCss": true,
      "enablePwa": true,
      "enablePush": false,
      "manifest": {
        "name": "Webpack 脚手架",
        "short_name": "脚手架",
        "description": "脚手架，基于 webpack 搭建。"
      }
    },
    "dev": {
      "port": 8000,
      "https": false,
      "historyApi": false,
      "devtool": "cheap-module-eval-source-map",
      "rpcMode": "mock",
      "rpcPrefix": {
        "inner": "https://[host]/[path]/"
      },
      "deploy": {
        "assetNameHash": false,
        "assetExtractCss": false,
        "enablePwa": false
      }
    },
    "test": {
      "pagePrefix": "https://[host]/[path]/",
      "assetPrefix": "https://[host]/[path]/",
      "rpcPrefix": {
        "inner": "https://[host]/[path]/"
      },
      "deploy": {}
    },
    "prod": {
      "pagePrefix": "https://[host]/[path]/",
      "assetPrefix": "https://[host]/[path]/",
      "rpcPrefix": {
        "inner": "https://[host]/[path]/"
      },
      "deploy": {}
    }
  },
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "http://git.weidai.work/liping/scaffold-webpack"
  },
  "homepage": "http://git.weidai.work/liping/scaffold-webpack",
