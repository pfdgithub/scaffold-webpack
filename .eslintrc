{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "amd": true,
    "es6": true,
    "worker": true,
    "serviceworker": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "arrow-body-style": 1,
    "arrow-parens": 1,
    "block-scoped-var": 1,
    "comma-dangle": 1,
    "default-case": 1,
    "eqeqeq": 1,
    "no-console": 1,
    "no-debugger": 1,
    "no-eq-null": 1,
    "no-eval": 1,
    "no-floating-decimal": 1,
    "no-implicit-globals": 1,
    "no-implied-eval": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "no-var": 1,
    "semi": 1,
    "unicode-bom": 1
  }
}